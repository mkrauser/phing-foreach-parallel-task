{
    "name": "mak/phing-foreach-parallel-task",
    "homepage": "https://github.com/mkrauser/phing-foreach-parallel-task",
    "description": "phing task to run sub-tasks in a foreach-loop in separate, parallel processes",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Matthias Krauser",
            "email": "matthias@krauser.eu"
        }
    ],
    "prefer-stable": true,
    "minimum-stability": "dev",
    "require": {
        "php": "^7.3 || ^7.4 || ^8.0 || ^8.1 || ^8.2",
        "ext-json": "*",
        "mehr-als-nix/parallel": "^1.0",
        "phing/phing": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "MaK\\Phing\\Task\\": "src/"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phing/phing-composer-configurator": true
        }
    }
}
